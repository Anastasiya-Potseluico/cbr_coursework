<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="coursework">
<class name="InterfaceDescription" table="interface_description">
	<id name="caseId" column="caseId">
	  <generator class="native"/>
	</id>
	<property name="carcassesType" column="carcassesType"/>
	<property name="mark" column="mark"/>
	<property name="TiresType" column="TiresType"/>
	<property name="track" column="track"/>
	<property name="weather" column="weather"/>
</class>
</hibernate-mapping>