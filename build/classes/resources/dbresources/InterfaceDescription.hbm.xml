<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false">
<class name="InterfaceWidgetRecommender.InterfaceDescription" table="Travel">

	<id name="caseId" column="caseId">
	  <generator class="native"/>
	</id>
	<property name="carcassesType" column="carcassesType"/>
	<property name="mark" column="mark"/>
	<property name="TiresType" column="TiresType"/>
	<property name="track" column="track"/>
	<property name="weather" column="weather"/>
</class>
</hibernate-mapping>